{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["DOM", "DOM.Iterable", "ES2022"],

    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",

    "strict": true,
    "skipLibCheck": true,

    "allowJs": true,
    "checkJs": false,
    "noEmit": true,

    "types": ["vite/client"]
  },
  "include": ["src"]
}
